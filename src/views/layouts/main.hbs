<body>

  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PK5ZV2M" height="0" width="0"
      style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->

  {{> navigationTabs }}

  {{> navigationDrawer }}

  <div class="mdc-drawer-scrim"></div>

  {{> myStoryMenu }}

  <a id="purchase-fab" href="/purchase-your-copy" style="z-index:150000;"
    class="mdc-fab app-fab--absolute progress-bar-click {{displayNone disableFAB}}"
    data-mdc-auto-init="MDCRipple">
    <div class="mdc-fab__ripple"></div>
    <span class="mdc-fab__label" style="display:none;">Purchase Your Copy</span>
    <span class="material-icons mdc-fab__icon">add_shopping_cart</span>
  </a>

  <div id="content">
    {{{body}}}
  </div>

  <footer>
    <br>
    <p class="no-margin">SUBSCRIBE AND STAY UP-TO-DATE</p>
    <br>

    <form method="POST" id="subscription-form">

      <label class="mdc-text-field mdc-text-field--outlined subscribe" data-mdc-auto-init="MDCTextField">
        <input name="firstName" id="firstName" class="mdc-text-field__input subscribe" type="text"
          aria-labelledby="my-label-id">
        <span class="mdc-notched-outline">
          <span class="mdc-notched-outline__leading"></span>
          <span class="mdc-notched-outline__notch">
            <span class="mdc-floating-label subscribe" id="firstName">First Name *</span>
          </span>
          <span class="mdc-notched-outline__trailing"></span>
        </span>
      </label>

      <label class="mdc-text-field mdc-text-field--outlined subscribe" data-mdc-auto-init="MDCTextField">
        <input name="lastName" id="lastName" class="mdc-text-field__input subscribe" type="text"
          aria-labelledby="my-label-id">
        <span class="mdc-notched-outline">
          <span class="mdc-notched-outline__leading"></span>
          <span class="mdc-notched-outline__notch">
            <span class="mdc-floating-label subscribe" id="lastName">Last Name *</span>
          </span>
          <span class="mdc-notched-outline__trailing"></span>
        </span>
      </label>

      <label class="mdc-text-field mdc-text-field--outlined subscribe" data-mdc-auto-init="MDCTextField">
        <input name="email" id="email" class="mdc-text-field__input subscribe" type="text"
          aria-labelledby="my-label-id">
        <span class="mdc-notched-outline">
          <span class="mdc-notched-outline__leading"></span>
          <span class="mdc-notched-outline__notch">
            <span class="mdc-floating-label subscribe" id="email">Email Address *</span>
          </span>
          <span class="mdc-notched-outline__trailing"></span>
        </span>
      </label>

      <button id="subscribe" class="subscribe-btn icon btn mdc-icon-button material-icons">double_arrow</button>

      <button id="subscribe2" class="subscribe-btn mdc-button">
        <div class="mdc-button__ripple"></div>
        <span class="mdc-button__label">Submit</span>
      </button>

      {{> snackbar type="success" message="Thank you for subscribing! I look forward to going on this journey with you!"}}

      {{> snackbar type="error" message="There was a problem with your submission. Please make sure you've entered all required fields."}}

    </form>

    <p id="footer-links">
      {{!-- <span>GOOD READS</span> --}}
      <span><a href="/contact-me" class="default-link icon default-icon">CONTACT ME</a></span>
      <span><a href="/contact-me?requestType=event" class="default-link icon default-icon">WANT ME TO VISIT
          YOU?</a></span>
    </p>

    {{> socialMediaLinks size=" fa-2x" classes="default-icon" element="p"}}

    <p id="policy-links">
      <span><a href="/privacy-policy" class="policy default-link icon default-icon">Privacy Policy</a></span>
      &nbsp;&nbsp;<span class="pipe">|</span>&nbsp;&nbsp;
      <span><a href="/credits" class="policy default-link icon default-icon">Credits</a></span>
    </p>

  </footer>

  {{!-- <script src="/js/dflip/js/libs/jquery.min.js" type="text/javascript"></script> --}}
  {{!-- <script src="/js/dflip/js/dflip.min.js" type="text/javascript"></script> --}}
  {{!-- <script type="text/javascript" src="/js/turn.js/turn.min.js"></script> --}}

  <script src="/javascripts/main.bundle.js" async></script>

</body>

</html>